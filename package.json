		"tailwindcss": "^4.1.17"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.15.3",
		"@eslint/js": "^9.33.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
